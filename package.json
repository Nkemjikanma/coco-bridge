{
  "name": "coco-bridge",
  "version": "0.1.0",
  "description": "AI agent for cross-chain bridging and token swaps on Towns Protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --noEmit",
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run src/__tests__/e2e-bridge-flow.test.ts"
  },
  "keywords": [
    "towns-protocol",
    "cross-chain",
    "bridge",
    "ai-agent",
    "crypto"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@towns-protocol/bot": "^1.0.3",
    "ioredis": "^5.4.1",
    "redis": "^5.10.0",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
